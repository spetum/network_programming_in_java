import java.util.*;  // for Properties

class ListProperties {

  public static void main(String args[]) {
	  //System.getProperties().list(System.out);   // Print all System properties
	  // System.out.println("\nFavorite Color: " +  // Print favorite color property
	  // System.getProperty("user.favoritecolor", "None"));
	  // file.separator
	  // path.separator
   String sepStr = new String(System.getProperties().getProperty("file.separator"));
   String pathStr = new String(System.getProperties().getProperty("path.separator"));
   System.out.println("File Separator : " + sepStr );
   System.out.println("File Separator : " + pathStr );
  }
}
